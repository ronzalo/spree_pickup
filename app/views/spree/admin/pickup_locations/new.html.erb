<% content_for :page_title do %>
  <%= Spree.t(:new_pickup_location) %>
<% end %>

<% content_for :page_actions do %>
  <li><%= link_to_with_icon 'arrow-left', Spree.t(:back_to_pickup_locations_list), admin_pickup_locations_path, :class => 'button' %></li>
<% end %>

<%= render :partial => 'spree/shared/error_messages', locals: { target: @pickup_location } %>

<%= form_for [:admin, @pickup_location] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>

